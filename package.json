{
  "name": "basic-nodejs-mitso",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": ""
  },
  "description": "nodejs-mitso",
  "main": "src/server.js",
  "scripts": {
    "start": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "lint": "eslint --ext .js,.ts --fix src/"
  },
  "keywords": [
    "rest",
    "node",
    "express",
    "autotest",
    "starter"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^9.0.2",
    "express": "^4.22.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.3.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.56.1",
    "debug": "^4.3.1",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.22",
    "prettier": "^2.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
